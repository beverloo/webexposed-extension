{
  "name": "WebExposed Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Extension to organize WebKit WebExposed bugs.",
  "permissions": [
    "http://webkit.org/"
  ],
  "content_scripts": [
    {
      "matches": ["https://bugs.webkit.org/buglist.cgi?keywords=WebExposed"],
      "js": ["webexposed.js"],
      "css": ["webexposed.css"]
    }
  ]
}
