{
  "name": "WebExposed Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Extension to organize WebKit WebExposed bugs.",
  "permissions": [
    "http://webkit.org/"
  ],
  "content_scripts": [
    {
      // Note: we cannot include the query parameter in the 'matches' string
      // because of crbug.com/57423
      "matches": ["https://bugs.webkit.org/buglist.cgi*"],
      "js": ["webexposed.js"],
      "css": ["webexposed.css"]
    }
  ]
}
